<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bola.wiradipa.org.lapanganbola">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Role_Activity"
            android:label="" />
        <activity
            android:name=".RegisterActivity"
            android:label="REGISTER" />
        <activity
            android:name=".ProfilActivity"
            android:label="EDIT PROFILE">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".AjakTandingActivity"
            android:label="AJAK TANDING">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".SewaLapanganActivity"
            android:label="SEWA LAPANGAN">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_navigation_bottom" />
        <activity
            android:name=".JadwalActivity"
            android:label="JADWAL TANDING">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".AkanDatangActivity"
            android:label="JADWAL TANDING">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".JadwalActivity" />
        </activity>
        <activity android:name=".LapanganActivity" />
        <activity android:name=".CariLapanganActivity"
            android:label="SEWA LAPANGAN">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SewaLapanganActivity" />
        </activity>
        <activity android:name=".PembayaranActivity"
            android:label="PEMBAYARAN">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DetailLapanganActivity" />
        </activity>
        <activity android:name=".DetailLapanganActivity"
            android:label="DETAIL LAPANGAN">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".CariLapanganActivity" />
        </activity>
    </application>
</manifest>